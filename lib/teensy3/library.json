{
    "name": "teensy36_FreeRTOS",
    "version": "0.0.0",
    "build": {
        "flags": [
        	"-I freertos/",
	        "-I freertos/include",
	        "-I freertos/portable/GCC/ARM_CM4F/",
    		"-I freertos/FreeRTOS-Plus-CLI"
    	],

    	"srcFilter": [
    		"+<freertos>",
    		"+<freertos/portable/GCC/ARM_CM4F>",
    		"+<freertos/FreeRTOS-Plus-CLI>",
        	"+<*.c>",
        	"+<*.cpp>",
        	"+<*.h>"
    	]
    }
}